{
  "name": "Hangouts Grid View",
  "description": "Get some gridd",
  "version": "0.1",
  "background": {
    "scripts": ["gridview-background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
    "matches": [
      "https://hangouts.google.com/*"
    ],
    "js": [
      "gridview-content.js"
    ],
    "run_at": "document_idle",
    "all_frames": false
  }
],

  "permissions": ["https://hangouts.google.com/"],
  "icons": {
    // "16": "icon-16.png",
    // "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    // "default_icon": "lcp-on.png",
    "default_title": "Google Hangouts Grid View - Click to toggle on / off"
  },
  "manifest_version": 2,
  "key": ""

}